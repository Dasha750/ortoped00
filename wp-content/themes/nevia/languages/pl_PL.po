msgid "Animate first layer"
msgstr ""

#: ../plugins/LayerSlider/add.php:582 ../plugins/LayerSlider/edit.php:591
msgid "If enabled, first layer will animate (slide in) instead of fading."
msgstr ""

#: ../plugins/LayerSlider/add.php:585 ../plugins/LayerSlider/edit.php:594
msgid "Random slideshow"
msgstr ""

#: ../plugins/LayerSlider/add.php:587 ../plugins/LayerSlider/edit.php:596
