msgstr ""

#: ../plugins/LayerSlider/add.php:567 ../plugins/LayerSlider/edit.php:576
msgid "If enabled, slideshow will automatically start after loading the page."
msgstr ""

#: ../plugins/LayerSlider/add.php:570 ../plugins/LayerSlider/edit.php:579
msgid "Pause on hover"
msgstr ""

#: ../plugins/LayerSlider/add.php:572 ../plugins/LayerSlider/edit.php:581
